<% include ./partials/header %>
	<div class="container">
		<div class="row">
			<div class="col-lg-12">
				<div id="content">
					<h1>Login </h1>
					<form action="/" method="Post">
						<div class="form-group">
							<input class="form-control clr" type="text" name="username" id="inputId" placeholder="UserName">
						</div>

						<div class="form-group">
							<input class="form-control clr" type="password" name="password" id="inputpassword" placeholder="Password">
						</div>

						<div class="form-group">
							<button type="submit" class="btn btn-block btn-outline-dark">Login</button>
						</div>	
					</form>
				</div>
			</div>
		</div>
	</div>

<script type="text/javascript" src="background.js"></script>

<% include ./partials/footer %>